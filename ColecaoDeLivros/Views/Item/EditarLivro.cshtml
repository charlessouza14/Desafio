@{
    var atualizar = ViewBag.Livro;
}
<title>Editar Biblioteca</title>
<form action="Put" method="post">
    
    <input type="hidden" value="@atualizar.Id" name="Id" id="nome" />   

    <input type="text" value="@atualizar.Nome" name="Fornecedor" id="nome" />
    <label for="nome">Fornecedor</label>
    <br /><br />
   
    <input type="text" value="@atualizar.Item" name="Recebedor" id="nome" />
    <label for="nome">Recebedor</label>  
    <br /><br />

    <input type="text" value="@atualizar.Status" name="Status" id="nome" />
    <label for="nome">Status</label>
    <br /><br />

    <input type="text" value="@atualizar.UltimaAtualizacao" name="DataEmprestimo" id="nome" />
    <label for="nome">DataEmprestimo</label>
    <br /><br />
    

    <button type="submit"> Atualizar</button>

</form>
